citHeader("To cite ndi in publications, please use:")

citEntry(entry = "manual",
  title         = "UKBBcleanR: Prepare electronic medical record data from the UK Biobank for time-to-event analyses",
  author        = personList(as.person("Alexander Depaulis"), as.person("Derek W. Brown")),
  publisher     = "GitHub",
  year          = "2022",
  url           = "https://github.com/machiela-lab/UKBBcleanR",
  
  textVersion = 
  paste("Alexander Depaulis & Derek W. Brown (2022).", 
        "Prepare electronic medical record data from the UK Biobank for time-to-event analyses.",
        "GitHub.",
        "Accessed by: https://github.com/machiela-lab/UKBBcleanR")
)
